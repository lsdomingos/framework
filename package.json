{
  "name": "@tscircle/framework",
  "version": "0.0.28-0",
  "description": "This package contains the core code of the tscircle framework.",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register -r tsconfig-paths/register **/*.spec.ts --exit",
    "migrate:latest": "node node_modules/knex/bin/cli.js --cwd --knexfile knexfile.ts migrate:latest",
    "migrate:rollback": "node node_modules/knex/bin/cli.js --cwd --knexfile knexfile.ts migrate:rollback",
    "migrate:make": "node node_modules/knex/bin/cli.js --cwd --knexfile knexfile.ts -x ts migrate:make",
    "build": "tsc",
    "publish:patch": "tsc && npm version patch && cp package.json /dist && cd dist && npm publish"
  },
  "repository": {},
  "author": "",
  "homepage": "",
  "dependencies": {
    "@hapi/joi": "^16.1.4",
    "@slynova/flydrive": "^0.3.1",
    "@types/chai": "^4.2.2",
    "@types/knex": "^0.14.26",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "aws-sdk": "^2.578.0",
    "es6-request": "^3.0.5",
    "esserializer": "^1.0.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "jwks-rsa": "^1.6.0",
    "knex": "^0.19.4",
    "mocha": "^6.2.1",
    "mysql": "^2.17.1",
    "serverless-http": "^2.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "ts-node": "^8.4.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "supertest": "^4.0.2",
    "tsconfig-paths": "^3.9.0"
  }
}
